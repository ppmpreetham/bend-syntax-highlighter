{
  "name": "bend-lang",
  "displayName": "Bend Language",
  "publisher": "ppmpreetham",
  "description": "Syntax highlighting for Bend programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ppmpreetham/bend-syntax-highlighter"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bend",
      "aliases": ["Bend", "bend"],
      "extensions": [".bend"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bend",
      "scopeName": "source.bend",
      "path": "./syntaxes/bend.tmLanguage.json"
    }]
  }
}